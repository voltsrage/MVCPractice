@{
    ViewBag.Title = "RequestExample";
    Layout = "~/Views/Shared/_LayoutView.cshtml";
}
<div class="col-md-6">
    <table class="table">
        <tr>
            <td>Url</td>
            <td>@ViewBag.Url</td>
        </tr>
        <tr>
            <td>PhysicalPath</td>
            <td>@ViewBag.PhysicalPath</td>
        </tr>
        <tr>
            <td>Path</td>
            <td>@ViewBag.Path</td>
        </tr>
        <tr>
            <td>BrowserType</td>
            <td>@ViewBag.BrowserType</td>
        </tr>
        <tr>
            <td>QueryString</td>
            <td>@ViewBag.QueryString</td>
        </tr>
        <tr>
            <td>HttpMethod</td>
            <td>@ViewBag.HttpMethod</td>
        </tr>
        <tr>
            <td>Headers</td>
            <td>@ViewBag.Headers</td>
        </tr>
    </table>
</div>




