@model FirstApp.Models.Product
@{
    ViewBag.Title = "Details";
}

<h2>Subject Details</h2>

<table class="table">
    <tr>
        <th>Subject ID</th>
        <td>@Model.SubjectId</td>
    </tr>
    <tr>
        <th>Subject Name</th>
        <td>@Model.SubjectName</td>
    </tr>
    <tr>
        <th>Passing Grade</th>
        <td>@Model.PassingGrade</td>
    </tr>
</table>

