@model List<EFDbFoirstApproachExample.Models.Category>
@{
    ViewBag.Title = "Index";
}

<h2>Categories</h2>
<table class="table">
    <tr>
        <td>Category ID</td>
        <td>Category Name</td>
    </tr>
    @foreach(var item in Model)
    {
    <tr>
        <td>@item.CategoryID</td>
        <td>@item.CategoryName</td>
    </tr>
    }
</table>